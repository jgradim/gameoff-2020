function _init()
 p=init_player()
 npcs={
  init_npc(jump,{1,0,12,5,8,9}),
  init_npc(glide,{8,11,10,15,12,13}),
 }
 init_bg_fxs()
 init_fxs()
 init_lights(0,0)
end

fps=30
function _update()
 --input
 player_btns={"‚¨ÖÔ∏è","‚û°Ô∏è","‚¨ÜÔ∏è"}
 for i=1,#player_btns do
  if btn(i-1) then
   fn=p[player_btns[i]]
   if (fn) fn(p,btnp(i-1))
  end
 end
 if btnp(üÖæÔ∏è) then
  if path.found then
   path:apply()
  else
   path:find(npcs[2],npcs[2],p)
  end
 end

 --updates
 update_player(p)
 path:update()
 foreach(npcs,update_npc)

 --fxs
 fire_fxs()
 update_bg_fxs()
 update_fxs()
end

function _draw()
