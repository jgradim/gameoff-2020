walk_accel=0.5
jump_accel=2.8

function init_player()
 return class(
  init_entity(8,8,2,3),{
   sp=1,
   flp=false,
   x=30*8,
   y=17*8,
   
   --{idle,run,jump,glide,fall}
   state="idle",
   prev_state="idle",

   ‚¨ÖÔ∏è=function(self)
    self.dx-=walk_accel
   end,
   
   ‚û°Ô∏è=function(self)
    self.dx+=walk_accel
   end,
    
   ‚¨ÜÔ∏è=double_jump
  })
end

function update_player(p)
 update_entity(p)

 --flip
 if p.dx<0 then
  p.flp=true
 elseif p.dx>0 then
  p.flp=false
 end
 
 --state
 p.prev_state=p.state
 p.state="idle"

 if p.glide then
  p.state="glide"
 else
  if p.dy==0 then
   if p.dx!=0 then
    p.state="run"
   end
  else
   if p.dy<-1 then
    p.state="jump"
   elseif p.dy>1 then
    p.state="fall"
   end
  end
 end

 --sprite
 if p.state=="idle" then
  p.sp=1
 elseif p.state=="run" then
  p.sp=2+(t()*10)%3
 elseif p.state=="jump" then
  p.sp=3
 elseif p.state=="glide" then
  p.sp=1
 elseif p.state=="fall" then
  p.sp=1
 end
end

function jump(p,tap)
 if (not tap) return

 if p.dy==0 then
  p.dy-=jump_accel
 end
end

function double_jump(p,tap)
 if (not tap) return

 if not p._j or p.dy==0 then
  p._j=0
 else
  p._j+=1
 end
 if p._j<2 then
  p.dy=-jump_accel
 end
end

function glide(p,_)
 if not p.glide and p.dy==0 then
  p.dy=-jump_accel
  return
 end

 if not p.glide and p.dy<0 then
  return
 end

 p.glide=true
 p.dy-=gravity+0.25
end

function draw_player(p)
 spr(p.sp,p.x,p.y,1,1,p.flp)
end

