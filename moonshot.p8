--kls "extends" super
function class(super,kls)
 kls.meta={__index=super}
 return setmetatable(
  kls,kls.meta
 )
end

--check if a contains b
function contains(a,b)
 --[[
 a={x,y,w,h}
 b={x,y,w,h}
 ]]
 return b.x>=a.x
 and b.x+b.w<=a.x+a.w
 and b.y>=a.y
 and b.y+b.h<=a.y+a.h
end

--check if obj collides with map
function collide_map(
 obj,aim,flag
)
 --[[
 obj={x,y,w,h}
 aim=‚¨ÖÔ∏è,‚û°Ô∏è,‚¨ÜÔ∏è,‚¨áÔ∏è
 flag=
  0-stands on (eg: floor)
  1-bumps into (eg: wall)
 ]]

 local x=obj.x
 local y=obj.y
 local w=obj.w
 local h=obj.h

 local x1=0
 local y1=0
 local x2=0
 local y2=0

 if aim=="‚¨ÖÔ∏è" then
  x1=x-1
  y1=y
  x2=x
  y2=y+h-1
 elseif aim=="‚û°Ô∏è" then
  x1=x+w-1
  y1=y
  x2=x+w
  y2=y+h-1
 elseif aim=="‚¨ÜÔ∏è" then
  x1=x+2
  y1=y-1
  x2=x+w-2
  y2=y
 elseif aim=="‚¨áÔ∏è" then
  x1=x+2
  y1=y+h
  x2=x+w-2
  y2=y+h
 end

 return flag_on(x1,y1,flag)
 or flag_on(x1,y2,flag)
 or flag_on(x2,y1,flag)
 or flag_on(x2,y2,flag)
end

function flag_on(x,y,flag)
 return fget(mget(x\8,y\8),flag)
end

--[[
--to use, prepend "-" above
function tostring(any)
 if type(any)~="table" then
  return tostr(any)
 end
 local str = "{"
 for k,v in pairs(any) do
  if (str~="{") str=str..","
  str..=tostring(k).."="..tostring(v)
 end
 return str.."}"
end
