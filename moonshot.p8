gravity=0.2
inertia=0.75

function init_entity(
 w,h,max_dx,max_dy
)
 return {
  x=0,
  y=0,
  w=w,
  h=h,
  dx=0,
  dy=0,
  max_dx=max_dx,
  max_dy=max_dy,
 }
end

function discmid(a,b,c,step)
  local v=mid(a,b,c)
  v-=sgn(v)*(v%step)
  if abs(v)<step then
   v=0
  end
  return v
end

function update_entity(e)
 --gravity/inertia
 e.dy+=gravity
 e.dx*=inertia

 --vertical map collisions
 if e.dy>0 then
  if collide_map(e,"‚¨áÔ∏è",0) then
   e.dy=0
   e.glide=false
   e.y-=((e.y+e.h+1)%8)-1
  end
 elseif e.dy<0 then
  if collide_map(e,"‚¨ÜÔ∏è",0) then
   e.dy=0
  end
 end

 --horizontal map collisions
 if e.dx<0 then
  if collide_map(e,"‚¨ÖÔ∏è",0) then
   e.dx=0
  end
 elseif e.dx>0 then
  if collide_map(e,"‚û°Ô∏è",0) then
   e.dx=0
  end
 end

 --apply acceleration
 e.x+=e.dx
 e.y+=e.dy

 --clamp acceleration
 e.dx=discmid(
  -e.max_dx,e.max_dx,
  e.dx,0x0.01
 )
 e.dy=discmid(
  -e.max_dy,e.max_dy,
  e.dy,0x0.01
 )
end

